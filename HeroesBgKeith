soundeffect.Volume = 1
wait(1.3)
local anim2 = Instance.new("Animation")
anim2.AnimationId = "rbxassetid://18273457215"


local playAnim2 = humanoid:LoadAnimation(anim2)
playAnim2:Play()
playAnim:Stop()
end)
